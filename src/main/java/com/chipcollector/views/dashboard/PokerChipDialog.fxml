<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="505.0"
            prefWidth="620.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.chipcollector.controllers.dashboard.PokerChipDialogController">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <right>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="250.0" minWidth="10.0" prefWidth="210.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="250.0" minHeight="10.0" prefHeight="250.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="250.0" minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="250.0" minHeight="10.0" prefHeight="250.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <VBox alignment="CENTER" maxWidth="200.0" prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <Label text="Front"/>
                        <Pane maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0" styleClass="image-border">
                            <children>
                                <ImageView fx:id="frontImage" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true"
                                           preserveRatio="true">
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </ImageView>
                            </children>
                        </Pane>
                    </children>
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </VBox>
                <VBox alignment="CENTER" maxWidth="200.0" prefHeight="200.0" prefWidth="200.0" spacing="5.0"
                      GridPane.rowIndex="2">
                    <children>
                        <Label text="Back"/>
                        <Pane maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0" styleClass="image-border">
                            <children>
                                <ImageView fx:id="backImage" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true"
                                           preserveRatio="true">
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </ImageView>
                            </children>
                        </Pane>
                    </children>
                    <GridPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </GridPane.margin>
                </VBox>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
        </GridPane>
    </right>
    <bottom>
        <ButtonBar id="buttonBar" VBox.vgrow="NEVER">
            <buttons>
                <Button fx:id="okButton" text="OK" ButtonBar.buttonData="RIGHT"/>
                <Button fx:id="cancelButton" onAction="#onCancelAction" text="Cancel" ButtonBar.buttonData="RIGHT"/>
            </buttons>
        </ButtonBar>
    </bottom>
    <center>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="500.0" minWidth="-Infinity" prefWidth="140.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="10.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="45.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="10.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="45.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="10.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="10.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="10.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="20.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="20.0" valignment="BOTTOM"
                                vgrow="NEVER"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="20.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="20.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="20.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="20.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="20.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="20.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="92.0" minHeight="10.0" prefHeight="92.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="92.0" minHeight="10.0" prefHeight="92.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="92.0" minHeight="10.0" prefHeight="92.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="92.0" minHeight="10.0" prefHeight="92.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="92.0" minHeight="10.0" prefHeight="92.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Casino" GridPane.valignment="BOTTOM"/>
                <Label text="Denom" GridPane.rowIndex="2" GridPane.valignment="BOTTOM"/>
                <ComboBox id="denomComboBox" fx:id="denomComboBox" prefWidth="200.0" GridPane.rowIndex="3"/>
                <Label text="Issue" GridPane.columnIndex="2" GridPane.rowIndex="2" GridPane.valignment="BOTTOM"/>
                <Label text="Year" GridPane.columnIndex="4" GridPane.rowIndex="2" GridPane.valignment="BOTTOM"/>
                <TextField id="issueTextField" fx:id="issueTextField" GridPane.columnIndex="2" GridPane.rowIndex="3"/>
                <TextField fx:id="yearTextField" GridPane.columnIndex="4" GridPane.rowIndex="3"/>
                <HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" prefHeight="25.0" prefWidth="397.0" spacing="5.0"
                      GridPane.columnSpan="9" GridPane.rowIndex="1">
                    <children>
                        <HBox id="casinoHBox" alignment="CENTER_LEFT" maxHeight="-Infinity" prefHeight="25.0"
                              spacing="2.0">
                            <children>
                                <Label id="casinoContent" fx:id="casinoContent" contentDisplay="RIGHT"
                                       maxHeight="-Infinity" prefHeight="25.0" prefWidth="500.0">
                                    <padding>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </padding>
                                </Label>
                                <ImageView fx:id="casinoCountryFlag" fitHeight="20.0" fitWidth="30.0"
                                           pickOnBounds="true" preserveRatio="true"/>
                            </children>
                            <padding>
                                <Insets right="5.0"/>
                            </padding>
                        </HBox>
                        <Button maxHeight="-Infinity" maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                                prefHeight="25.0" prefWidth="25.0" text="+"/>
                    </children>
                </HBox>
                <Label text="Color" GridPane.rowIndex="4" GridPane.valignment="BOTTOM"/>
                <ComboBox fx:id="colorComboBox" prefWidth="300.0" GridPane.rowIndex="5"/>
                <Label alignment="BOTTOM_LEFT" text="Mold" GridPane.columnIndex="2" GridPane.rowIndex="4"
                       GridPane.valignment="BOTTOM"/>
                <ComboBox fx:id="moldComboBox" prefWidth="300.0" GridPane.columnIndex="2" GridPane.columnSpan="7"
                          GridPane.rowIndex="5"/>
                <ComboBox fx:id="inlayComboBox" prefWidth="200.0" GridPane.rowIndex="7"/>
                <Label text="Inlay" GridPane.rowIndex="6" GridPane.valignment="BOTTOM"/>
                <Label text="Inserts" GridPane.columnIndex="2" GridPane.rowIndex="6" GridPane.valignment="BOTTOM"/>
                <TextField fx:id="insertsTextField" GridPane.columnIndex="2" GridPane.columnSpan="7"
                           GridPane.rowIndex="7"/>
                <Label text="Condition" GridPane.columnIndex="6" GridPane.columnSpan="3" GridPane.rowIndex="8"
                       GridPane.valignment="BOTTOM"/>
                <Label text="Category" GridPane.rowIndex="8" GridPane.valignment="BOTTOM"/>
                <ComboBox fx:id="conditionComboBox" prefWidth="200.0" GridPane.columnIndex="6" GridPane.columnSpan="3"
                          GridPane.rowIndex="9"/>
                <ComboBox fx:id="categoryComboBox" prefWidth="300.0" GridPane.rowIndex="9"/>
                <Label text="Rarity" GridPane.columnIndex="2" GridPane.rowIndex="8" GridPane.valignment="BOTTOM"/>
                <ComboBox fx:id="rarityComboBox" prefWidth="200.0" GridPane.columnIndex="2" GridPane.columnSpan="3"
                          GridPane.rowIndex="9"/>
                <Label text="TCR #" GridPane.columnIndex="6" GridPane.columnSpan="3" GridPane.rowIndex="2"
                       GridPane.valignment="BOTTOM"/>
                <TextField fx:id="valueTextField" GridPane.columnIndex="2" GridPane.columnSpan="4"
                           GridPane.rowIndex="11"/>
                <Label text="Date of Acquisition" GridPane.rowIndex="10" GridPane.valignment="BOTTOM"/>
                <Label text="Notes" GridPane.rowIndex="12"/>
                <DatePicker fx:id="dateOfAcquisitionDatePicker" GridPane.rowIndex="11"/>
                <Label text="Value" GridPane.columnIndex="2" GridPane.rowIndex="10" GridPane.valignment="BOTTOM"/>
                <TextField fx:id="tcrTextField" GridPane.columnIndex="6" GridPane.columnSpan="3" GridPane.rowIndex="3"/>
                <TextArea fx:id="notesTextArea" prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="8"
                          GridPane.rowIndex="13" GridPane.rowSpan="7"/>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="5.0" GridPane.columnIndex="8"
                      GridPane.rowIndex="17" GridPane.rowSpan="3" GridPane.valignment="BOTTOM">
                    <children>
                        <ToggleButton id="obsoleteToggleButton" fx:id="obsoleteToggleButton" focusTraversable="false"
                                      maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                      minWidth="-Infinity" mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0"/>
                        <ToggleButton id="cancelledToggleButton" fx:id="cancelledToggleButton" focusTraversable="false"
                                      maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                      minWidth="-Infinity" mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0"/>
                    </children>
                </VBox>
                <Label text="Paid" GridPane.columnIndex="7" GridPane.columnSpan="2" GridPane.rowIndex="10"/>
                <TextField fx:id="paidTextField" GridPane.columnIndex="7" GridPane.columnSpan="2"
                           GridPane.rowIndex="11"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </GridPane>
    </center>
    <stylesheets>
        <URL value="@PokerChipDialog.css"/>
    </stylesheets>
</BorderPane>
