<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.cell.*?>
<?import javafx.scene.control.*?>
<TableView fx:id="pokerChipTableView" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <columns>
        <TableColumn maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Front">
            <cellValueFactory>
                <PropertyValueFactory property="frontImageThumbnailView"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Back">
            <cellValueFactory>
                <PropertyValueFactory property="backImageThumbnailView"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn prefWidth="75.0" text="Casino">
            <cellValueFactory>
                <PropertyValueFactory property="casinoName"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn prefWidth="75.0" text="City">
            <cellValueFactory>
                <PropertyValueFactory property="casinoCity"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn prefWidth="75.0" text="Denom">
            <cellValueFactory>
                <PropertyValueFactory property="denom"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn prefWidth="75.0" text="Mold">
            <cellValueFactory>
                <PropertyValueFactory property="mold"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn prefWidth="75.0" text="Color">
            <cellValueFactory>
                <PropertyValueFactory property="color"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn prefWidth="75.0" text="Inlay">
            <cellValueFactory>
                <PropertyValueFactory property="inlay"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn prefWidth="75.0" text="Inserts">
            <cellValueFactory>
                <PropertyValueFactory property="inserts"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn prefWidth="75.0" text="Year">
            <cellValueFactory>
                <PropertyValueFactory property="year"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn prefWidth="75.0" text="TCR ID">
            <cellValueFactory>
                <PropertyValueFactory property="tcrId"/>
            </cellValueFactory>
        </TableColumn>
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
    </columnResizePolicy>
</TableView>
